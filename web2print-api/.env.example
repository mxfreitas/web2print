# Web2Print API Configuration
# Copie este arquivo para .env e preencha com seus valores

# API Security
API_KEY=sua_chave_api_aqui
WEB2PRINT_API_KEY=sua_chave_api_aqui

# Flask Configuration
FLASK_ENV=development
SECRET_KEY=sua_chave_secreta_flask_aqui

# Database
SQLALCHEMY_DATABASE_URI=sqlite:///users.db

# PDF Processing Configuration
MAX_SYNC_PDF_SIZE=10485760
MAX_PDF_SIZE_TOTAL=52428800
HEALTH_CHECK_TIMEOUT=3
PDF_DOWNLOAD_TIMEOUT=30
ENABLE_SIZE_PRECHECK=true
CLEANUP_LOG_LEVEL=INFO

# Security & Production
HTTPS_ENABLED=false

# Deployment
PORT=5000
HOST=0.0.0.0